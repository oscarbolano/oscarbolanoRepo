let setValores = new Set("1",2,3,"4",5,"6",7,8,"9","0");
let setNumeros = new Set();
class Valida{
    numeros(valor){
        for(let valor of setValores){
            if(!isNaN(valor));{
                setNumeros.add(valor);
            }
        }
    }
}
let valida = new Valida();
valida.numeros();
console.log("Los valores numericos son: "+setNumeros);
